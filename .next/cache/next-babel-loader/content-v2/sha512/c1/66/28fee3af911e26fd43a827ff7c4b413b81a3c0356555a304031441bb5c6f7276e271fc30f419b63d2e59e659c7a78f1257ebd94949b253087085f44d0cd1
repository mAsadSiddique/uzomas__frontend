    <Product product={item} key={item.id} />\r\n            ));\r\n            if (layout !== 'standard') {\r\n                productItemsView = (\r\n                    <Slider\r\n                        ref={slider => sliderRef.current = slider}\r\n                        {...carouselFullwidth}\r\n                        arrows={false}\r\n                        className=\"ps-carousel outside\">\r\n                        {slideItems}\r\n                    </Slider>\r\n                );\r\n            } else {\r\n                productItemsView = (\r\n                    <Slider\r\n                        ref={slider => sliderRef.current = slider}\r\n                        {...carouselStandard}\r\n                        arrows={false}\r\n                        className=\"ps-carousel outside\">\r\n                        {slideItems}\r\n                    </Slider>\r\n                );\r\n            }\r\n\r\n        } else {\r\n            productItemsView = <p>No product found.</p>;\r\n        }\r\n    } else {\r\n        const skeletons = generateTempArray(6).map((item) => (\r\n            <div className=\"col-xl-2 col-lg-3 col-sm-3 col-6\" key={item}>\r\n                <SkeletonProduct />\r\n            </div>\r\n        ));\r\n        productItemsView = <div className=\"row\">{skeletons}</div>;\r\n    }\r\n\r\n    return (\r\n        <div className=\"ps-block--shop-features\">\r\n            <div className=\"ps-block__header\">\r\n                <h3>{title}</h3>\r\n                <div className=\"ps-block__navigation\">\r\n                    <a\r\n                        className=\"ps-carousel__prev\"\r\n                        onClick={(e) => handleCarouselPrev(e)}>\r\n                        <i className=\"icon-chevron-left\"></i>\r\n                    </a>\r\n                    <a\r\n                        className=\"ps-carousel__next\"\r\n                        onClick={(e) => handleCarouselNext(e)}>\r\n                        <i className=\"icon-chevron-right\"></i>\r\n                    </a>\r\n                </div>\r\n            </div>\r\n            <div className=\"ps-block__content\">\r\n                {productItemsView}\r\n            </div>\r\n        </div>\r\n    );\r\n};\r\n\r\nexport default ProductGroupByCarousel;\r\n"]},"metadata":{},"sourceType":"module"}